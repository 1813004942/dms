<x:beans xmlns:x="http://www.springframework.org/schema/beans"
	xmlns="http://www.bocom.com/schema/jump-core" xmlns:osgi="http://www.springframework.org/schema/osgi"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
						http://www.springframework.org/schema/osgi http://www.springframework.org/schema/osgi/spring-osgi-2.0-m1.xsd
						http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-3.0.xsd
						http://www.bocom.com/schema/jump-core http://www.bocom.com/schema/jump-core-1.0.xsd">
	
	<process id="selectData" template="defaultTemplate" interceptors="defaultInterceptors">
		<actions>
			<action id="defaultTemplateAction" ref="demoAction" operations="selectData"></action>
		</actions>
	</process>
	
	<!-- 查询项目列表 -->
	<process id="queryDemoList" template="defaultTemplate" interceptors="defaultInterceptors">
		<actions>
			<action id="defaultTemplateAction" ref="demoAction" operations="queryDemoList"></action>
		</actions>
	</process>
	
	<!-- 查询项目明细 -->
	<process id="queryDemoDetail" template="defaultTemplate" interceptors="defaultInterceptors">
		<actions>
			<action id="defaultTemplateAction" ref="demoAction" operations="queryDemoDetail"></action>
		</actions>
	</process>
	
	<!-- 新增项目 -->
	<process id="addDemo" template="defaultTemplate" interceptors="defaultInterceptors">
		<actions>
			<action id="defaultTemplateAction" ref="demoAction" operations="addDemo"></action>
		</actions>
	</process>
	
	<!-- 修改项目 -->
	<process id="updateDemo" template="defaultTemplate" interceptors="defaultInterceptors">
		<actions>
			<action id="defaultTemplateAction" ref="demoAction" operations="updateDemo"></action>
		</actions>
	</process>
	
	<!-- 删除项目 -->
	<process id="deleteDemo" template="defaultTemplate" interceptors="defaultInterceptors">
		<actions>
			<action id="defaultTemplateAction" ref="demoAction" operations="deleteDemo"></action>
		</actions>
	</process>
	
</x:beans>
